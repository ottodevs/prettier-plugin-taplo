{
  "name": "@ottodevs/prettier-plugin-taplo",
  "version": "1.0.5",
  "packageManager": "pnpm@8.7.5",
  "author": "Otto G <5030059+ottodevs@users.noreply.github.com> (https://github.com/ottodevs)",
  "description": "Use taplo to format your TOML files with prettier.",
  "keywords": [
    "toml",
    "taplo",
    "prettier",
    "format",
    "formatter"
  ],
  "homepage": "https://github.com/ottodevs/prettier-plugin-taplo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ottodevs/prettier-plugin-taplo.git"
  },
  "bugs": {
    "url": "https://github.com/ottodevs/prettier-plugin-taplo/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    },
    "./worker": {
      "require": "./dist/worker.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "@ottodevs:registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "scripts": {
    "build": "pkgroll",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "watch": "pkgroll --watch"
  },
  "dependencies": {
    "@taplo/lib": "0.4.0-alpha.2",
    "synckit": "0.8.5"
  },
  "devDependencies": {
    "@antfu/ni": "0.21.8",
    "@so1ve/eslint-config": "0.121.4",
    "@types/node": "20.6.2",
    "bumpp": "9.2.0",
    "eslint": "8.49.0",
    "pkgroll": "1.11.0",
    "prettier": "3.0.3",
    "tsx": "3.12.10",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vitest": "0.34.4"
  },
  "peerDependencies": {
    "prettier": "^3.0.3"
  },
  "directories": {
    "test": "test"
  }
}
